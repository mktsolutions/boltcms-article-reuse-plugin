  "cta-banner": {
    "name": "CTA banner with photo",
    "html": "<section class=\"cta-banner-section\">\n<div class=\"row\">\n<div class=\"col-12 col-md-6\">\n<div class=\"cta-banner-section__content\">\n<p class=\"cta-banner-text text-22\">\nCreate the advanced user experience of software-defined vehicles\n</p>\n\t\t<div class=\"btn-container\">\n<a href=\"#\" class=\"btn-arrow-ghost--rich-black outline transparent\">read more<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"25\" viewBox=\"0 0 24 25\" fill=\"none\"><path d=\"M16.2581 8.2417L6.96777 17.532\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path><path d=\"M10 7.5L16.95 7.549L17 14.5\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path></svg>\n\t\t</a>\n</div>\n      </div>\n    </div>\n\t<div class=\"col-12 col-md-6 d-none d-md-block\">\n      <div class=\"cta-banner-section__image\">\n<figure>\n<img src=\"/theme/luxoft-2024/assets/images/others/blog-cta-with-banner.png\" alt=\"cta banner\"></figure>\n\t</div>\n</div></div></section>"
  },
  "photo-container-simple": {
    "name": "Blog banner",
    "html": "<div class=\"blog-photo-banner mb-3\">\n<figure class=\"blog-photo-banner__figure\">\n<img class=\"blog-photo-banner__figure--img\" src=\"/theme/luxoft-2024/assets/images/others/blog-banner.jpg\">\n</figure>\n</div>"
  },
  "photo-lead-paragraph": {
    "name": "Blog lead paragraph",
    "html": "<div class=\"blog-lead-paragraph mb-3\">\n<p>A key requirement for effectively managing legacy applications is to reduce discretionary demand and fulfil only mandatory demand. This requires a robust demand management function to continuously drive demand optimization.</p>\n</div>"
  },
  "location-block-cards": {
    "name": "Location block cards",
    "html": "<div class=\"location-block-cards\">\n<div class=\"location-block-cards__card\">\n<div class=\"location-block-cards__card__title\">\n<h3>Warsaw</h3>\n</div>\n<div class=\"location-block-cards__card__location-details\">\n<p class=\"text-dark-gray\">Location</p>\n<p>Warsaw Spire, plac Europejski 1, 00-844 Warszawa</p>\n</div>\n<div class=\"location-block-cards__card__email\">\n<p class=\"text-dark-gray\">Email</p>\n<p>it-trainings@luxoft.com</p>\n</div>\n<div class=\"location-block-cards__card__phone\">\n<p class=\"text-dark-gray\">Phone</p>\n<p>+48 122110666</p>\n</div>\n<div class=\"location-block-cards__card__view-btn\">\n<a href=\"http://maps.google.com/?q=Warsaw%20Luxoft%20Warsaw%20-%20Warsaw%20Spire,%20plac%20Europejski%201,%2000-844%20Warszawa\" target=\"_blank\" class=\"btn-arrow-ghost--purple outline text-uppercase\">\nView on map\n<svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"25\" viewBox=\"0 0 24 25\" fill=\"none\"><path d=\"M16.2581 8.2417L6.96777 17.532\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path><path d=\"M10 7.5L16.95 7.549L17 14.5\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path></svg>\n                </a>\n            </div>\n        </div>\n        <div class=\"location-block-cards__card\">\n            <div class=\"location-block-cards__card__title\">\n                <h3>Bucharest</h3>\n            </div>\n            <div class=\"location-block-cards__card__location-details\">\n                <p class=\"text-dark-gray\">Location</p>\n                <p>Dimitrie Pompeiu nr 5-7 , building C, Et. 5, sect 2, Bucharest, 014459</p>\n            </div>\n            <div class=\"location-block-cards__card__email\">\n                <p class=\"text-dark-gray\">Email</p>\n                <p>it-trainings@luxoft.com</p>\n            </div>\n            <div class=\"location-block-cards__card__phone\">\n                <p class=\"text-dark-gray\">Phone</p>\n                <p>+40 021 371 4858</p>\n            </div>\n            <div class=\"location-block-cards__card__view-btn\">\n                <a href=\"http://maps.google.com/?q=Bucharest%20Dimitrie%20Pompeiu%20nr%205-7%20,%20building%20C,%20Et.%205,%20sect%202,%20Bucharest,%20014459\" target=\"_blank\" class=\"btn-arrow-ghost--purple outline text-uppercase\">\n                    View on map\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"25\" viewBox=\"0 0 24 25\" fill=\"none\"><path d=\"M16.2581 8.2417L6.96777 17.532\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path><path d=\"M10 7.5L16.95 7.549L17 14.5\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path></svg>\n                </a>\n            </div>\n        </div>\n        <div class=\"location-block-cards__card\">\n            <div class=\"location-block-cards__card__title\">\n                <h3>Wroclaw</h3>\n            </div>\n            <div class=\"location-block-cards__card__location-details\">\n                <p class=\"text-dark-gray\">Location</p>\n                <p>Silver Tower pl. Konstytucji 3-go Maja 3 50-048 Wroclaw</p>\n            </div>\n            <div class=\"location-block-cards__card__email\">\n                <p class=\"text-dark-gray\">Email</p>\n                <p>it-trainings@luxoft.com</p>\n            </div>\n            <div class=\"location-block-cards__card__phone\">\n                <p class=\"text-dark-gray\">Phone</p>\n                <p>+48 122110666</p>\n            </div>\n            <div class=\"location-block-cards__card__view-btn\">\n                <a href=\"http://maps.google.com/?q=Wroclaw%20Luxoft%20Poland%20Wroclaw%20-%20Silver%20Tower%20pl.%20Konstytucji%203-go%20Maja%203%2050-048%20Wroclaw\" target=\"_blank\" class=\"btn-arrow-ghost--purple outline text-uppercase\">\n                    View on map\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"25\" viewBox=\"0 0 24 25\" fill=\"none\"><path d=\"M16.2581 8.2417L6.96777 17.532\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path><path d=\"M10 7.5L16.95 7.549L17 14.5\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path></svg>\n                </a>\n            </div>\n        </div>\n        <div class=\"location-block-cards__card\">\n            <div class=\"location-block-cards__card__title\">\n                <h3>Krakow</h3>\n            </div>\n            <div class=\"location-block-cards__card__location-details\">\n                <p class=\"text-dark-gray\">Location</p>\n                <p>Aleja Generała Tadeusza Bora-Komorowskiego 25, Quattro Business Park Five, 31-476 Kraków, Poland</p>\n            </div>\n            <div class=\"location-block-cards__card__email\">\n                <p class=\"text-dark-gray\">Email</p>\n                <p>it-trainings@luxoft.com</p>\n            </div>\n            <div class=\"location-block-cards__card__phone\">\n                <p class=\"text-dark-gray\">Phone</p>\n                <p>+48 122110666</p>\n            </div>\n            <div class=\"location-block-cards__card__view-btn\">\n                <a href=\"http://maps.google.com/?q=Krakow%20Aleja%20Genera%C5%82a%20Tadeusza%20Bora-Komorowskiego%2025,%20Quattro%20Business%20Park%20Five,%2031-476%20Krak%C3%B3w,%20Poland\" target=\"_blank\" class=\"btn-arrow-ghost--purple outline text-uppercase\">\n                    View on map\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"25\" viewBox=\"0 0 24 25\" fill=\"none\"><path d=\"M16.2581 8.2417L6.96777 17.532\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path><path d=\"M10 7.5L16.95 7.549L17 14.5\" stroke-width=\"1.5\" stroke-linecap=\"square\" stroke-linejoin=\"round\"></path></svg>\n                </a>\n            </div>\n        </div>\n    </div>"
